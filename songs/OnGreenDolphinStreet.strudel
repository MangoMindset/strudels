let bassA = n(bassline)
  .scale(scaleA)
  .lastOf(8, x => x.hpf("1500").hpenv(2))

let bassB = n(bassline.slow(2))
  .scale(scaleB)
  .lastOf(4, x => x.hpf("<1000!4 1500!4>").hpenv("4!4 2!4"))

